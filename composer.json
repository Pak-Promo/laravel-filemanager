{
    "name": "pakpromo/laravel-filemanager",
    "description": "File Manager for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "PakPromo",
            "email": "snippetcms@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0",
        "intervention/image-laravel": "^1.3"
    },
    "require-dev": {
        "ext-imagick": "*",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "PakPromo\\FileManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PakPromo\\FileManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PakPromo\\FileManager\\FileManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
